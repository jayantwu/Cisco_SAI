        </List>
    </Config>
    <Files>
        <Folder
            Name="Source Files"
            Filters="*.c;*.C;*.cc;*.cpp;*.cp;*.cxx;*.c++;*.prg;*.pas;*.dpr;*.asm;*.s;*.bas;*.java;*.cs;*.sc;*.e;*.cob;*.html;*.rc;*.tcl;*.py;*.pl;*.d;*.m;*.mm;*.go"
            GUID="{63ED2468-C2FA-409D-9789-511C82522A5B}">
            <F
                N="../src/common/*.cpp"
                Recurse="1"
                Excludes=""/>
        </Folder>
        <Folder
            Name="Header Files"
            Filters="*.h;*.H;*.hh;*.hpp;*.hxx;*.h++;*.inc;*.sh;*.cpy;*.if"
            GUID="{2E5A57B5-D11B-4CCC-B7E2-DCAA3459D30A}">
            <F
                N="../include/common/*.h"
                Recurse="1"
                Excludes=""/>
        </Folder>
        <Folder
            Name="Other Files"
            Filters=""
            GUID="{3FD70974-A53B-4D85-B347-ABE38F6931CB}">
            <F
                N="../include/common/*.i"
                Recurse="1"
                Excludes=""/>
            <F
                N="../src/common/Makefile*"
                Recurse="1"/>
            <F
                N="../test/common/Makefile*"
                Recurse="1"/>
        </Folder>
    </Files>
</Project>
